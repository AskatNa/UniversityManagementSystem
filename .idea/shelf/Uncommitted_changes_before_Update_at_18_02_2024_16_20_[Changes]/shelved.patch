tring toString() {\r\n        return getName() +\" \" + getSurname() +\" \" + \"Attendence:\" + Attendence + \" \" + \"Retake:\" +DeadSouls() + \" Gpa: \" + getGpa();\r\n    }\r\n}\r\n
