          }\r\n        } catch (ClassNotFoundException | SQLException e) {\r\n            e.printStackTrace();\r\n        } finally {\r\n            try {\r\n                if (resultSet != null) {\r\n                    resultSet.close();\r\n                }\r\n                if (preparedStatement != null) {\r\n                    preparedStatement.close();\r\n                }\r\n                if (stmt != null) {\r\n                    stmt.close();\r\n                }\r\n                if (con != null) {\r\n                    con.close();\r\n                }\r\n                scanner.close();\r\n            } catch (SQLException e) {\r\n                e.printStackTrace();\r\n            }\r\n        }\r\n    }\r\n}\r\n\r\n
